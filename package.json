{
  "name": "my-qwik-empty-starter",
  "description": "Blank project with routing included",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build.client && npm run build.server && npm run build.copy",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/express/vite.config.ts",
    "build.copy": "mkdir -p server && cp -r .adapters/express/* server/",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "node server/entry.express.js",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.express.js",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.18.0",
    "@builder.io/qwik-city": "^1.18.0",
    "@eslint/js": "latest",
    "@qwikdev/pwa": "^0.0.4",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "20.19.0",
    "@types/sql.js": "^1.4.9",
    "autoprefixer": "^10.4.23",
    "eslint": "9.32.0",
    "eslint-plugin-qwik": "^1.18.0",
    "globals": "16.4.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.18",
    "typescript": "5.4.5",
    "typescript-eslint": "8.38.0",
    "typescript-plugin-css-modules": "latest",
    "undici": "*",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.6",
    "compression": "^1.8.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "form-data": "^4.0.5",
    "http-proxy-middleware": "^3.0.5",
    "mailgun.js": "^12.4.1",
    "pocketbase": "^0.26.5",
    "quill": "^2.0.3",
    "sql.js": "^1.13.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  }
}
